{
    "name": "bookit/codeception-testrail",
    "description": "Codeception Extension for TestRail Reporting",
    "homepage": "https://github.com/bookitcom",
    "license": "MIT",
    "require": {
        "codeception/codeception": "^4.0",
        "guzzlehttp/guzzle": "^6.2",
        "ext-json": "*"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "squizlabs/php_codesniffer": "^2.6",
        "codeception/module-phpbrowser": "^1.0",
        "codeception/module-asserts": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "BookIt\\Codeception\\TestRail\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BookIt\\Codeception\\TestRail\\Tests\\Unit\\": "tests/unit"
        }
    }
}
